{
  "name": "pubg-discord-bot-api",
  "private": true,
  "version": "0.0.1",
  "description": "A discord bot to view stats and overview from PUBG",
  "main": "main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "build": "webpack --config webpack.$NODE_ENV.js",
    "start": "node dist/server",
    "start:env": "node --require dotenv/config dist/server",
    "watch": "tsc -p tsconfig.json -w",
    "test": "mocha -r ts-node/register \"tests/**/*.spec.ts\"",
    "generate": "graphql-codegen",
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test",
    "clean": "rm -rf dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/megacat-studio/pubg-discord-bot.git"
  },
  "keywords": [
    "mocha",
    "typescript",
    "heroku",
    "discord.js"
  ],
  "author": "megacat-studio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/megacat-studio/pubg-discord-bot/issues"
  },
  "homepage": "https://github.com/megacat-studio/pubg-discord-bot#readme",
  "dependencies": {
    "apollo-server": "^2.9.7",
    "graphql": "^14.5.8",
    "reflect-metadata": "^0.1.13",
    "tslint": "^5.20.0",
    "tslint-config-airbnb": "^5.11.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.8.2",
    "@graphql-codegen/typescript": "^1.8.2",
    "@types/webpack-env": "^1.14.1",
    "clean-webpack-plugin": "^3.0.0",
    "dotenv": "^8.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
